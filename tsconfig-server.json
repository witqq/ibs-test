{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "noImplicitAny": false,
    "outDir": "dist",
    "lib": [
      "dom",
      "es5",
      "es6",
      "es2015.collection"
    ],
    "experimentalDecorators": true
  },
  "include": [
    "server/**/*",
    "share/**/*"
  ],
  "exclude": [
    "node_modules",
    "target",
    "ts-compile",
    "client/**/*"
  ]
}
